digraph unix {
	rankdir=LR size="8,5"
	"Pila: B#\nEntrada: a" [shape=box]
	"" [height=0.0001 shape=plaintext witdh=0.0001]
	i [shape=circle]
	p [shape=circle]
	q [color=yellow height=1 shape=circle style=filled witdh=1]
	f [shape=doublecircle]
	"" -> i
	i -> p [label="λ,λ;#"]
	p -> q [label="λ,λ;A"]
	q -> q [label="λ,A;B\nλ,B;aC <---- \nλ,C;aCbb\nλ,C;bb\n"]
	q -> q [label="a,a;λ \nb,b;λ \n" headport=sw tailport=s]
	q -> f [label="λ,#;λ"]
}
